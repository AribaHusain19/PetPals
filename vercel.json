
{
    "version": 2,
    "builds": [
      {
        "src": "Server/client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Serverclient/build"
        }
      },
      {
        "src": "Server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/Server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }
  
